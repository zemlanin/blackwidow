{
  "container": {
    "size": [16, 9]
  },
  "widgets": {
    "ready": {
      "type": "image",
      "container": {
        "position": [0, 0],
        "size": [1.5, 1.5],
        "debug": "green"
      },
      "endpoint": {
        "url": "https://bwd.cat/text/ready",
        "schedule": {
          "type": ["timeInterval"],
          "timeInterval": 300
        },
        "map": {
          "src": {
            "_format": "https://blackwidow.parseapp.com/{}.gif",
            "_path": "text"
          }
        }
      }
    },
    "duty": {
      "type": "text",
      "container": {
        "position": [2, 0],
        "size": [7, 1.5],
        "debug": "green"
      },
      "endpoint": {
        "url": "https://bwd.cat/text/duty",
        "schedule": {
          "type": ["timeInterval"],
          "timeInterval": 3600
        }
      },
      "data": {
        "note": "duty"
      }
    },
    "defaultVersion": {
      "type": "text",
      "container": {
        "position": [9, 0],
