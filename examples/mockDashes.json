    "qa-graph": {
      "type": "graph",
      "container": {
        "position": [14, 0],
        "size": [2, 2],
        "debug": "orange"
      },
      "endpoint": {
        "method": "GET",
        "url": "https://gist.githubusercontent.com/zemlanin/519918508bc9b0e9fe60/raw/ac8abcf6d3c6a294fa62cbb835792eff7892306d/qa.json",
        "schedule": {
          "type": ["timeInterval"],
          "timeInterval": 600
        },
        "map": {
          "values": {
            "_path": "stats",
            "_map": {
              "value": {
                "_path": "points",
                "_parseInt": true
              }
            }
          }
        }
      },
      "data": {
        "style": "line-chart",
        "baseValues": {
          "min": 0,
          "max": 800
        },
        "lines": [
          {"y": 0},
          {"y": 800}
        ],
        "sortBy": "created",
        "color": "white",
        "note": "bugpoints",
        "labelStyle": "none"
      }
    },
